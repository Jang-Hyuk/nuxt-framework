{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:debug": "nodemon --inspect",
    "staging": "cross-env NODE_ENV=staging nodemon",
    "prod": "cross-env NODE_ENV=production nodemon",
    "start": "node",
    "gen": "nodemon src/utils/procedureInterfaceMaker.js --ignore src/config/code/procedureTuples/*.js",
    "gen:popup": "start nodemon src/utils/procedureInterfaceMaker.js && start yarn lint:w",
    "lint": "eslint --ext \".js,.ts,.ts\" --ignore-path ../../.gitignore .",
    "lint:fix": "yarn lint --fix",
    "lint:w": "esw --ext \".js,.ts,.ts\" --ignore-path ../../.gitignore . -w --color",
    "lint:wf": "yarn lint:w --fix",
    "test:w": "jest --watch",
    "test": "jest --detectOpenHandles",
    "test:c": "jest --detectOpenHandles --coverage",
    "test:cw": "jest --detectOpenHandles --coverage --watch",
    "test:cwf": "jest --detectOpenHandles --coverage --watch --runTestsByPath",
    "tsc": "tsc -w --noEmit -p tsconfig.tsc.json",
    "pm2": "pm2 kill && cross-env NODE_ENV=production pm2 start ecosystem.config.js",
    "pm2:staging": "pm2 kill && cross-env NODE_ENV=staging pm2 start ecosystem.config.js",
    "pm2:dev": "pm2 kill && cross-env NODE_ENV=development pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:monit": "pm2 monit",
    "pm2:log": "pm2 logs",
    "pm2:kill": "pm2 kill"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jsdoc-swagger": "^1.6.7",
    "express-oas-validator": "^2.0.2",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "iconv-lite": "^0.6.3",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.2"
  },
  "devDependencies": {}
}
