{
  "name": "frontend",
  "version": "1.0.0",
  "description": "inforex renewal",
  "main": "app.js",
  "scripts": {
    "dev": "nuxt",
    "dev:debug": "node --inspect ../../node_modules/nuxt/bin/nuxt.js",
    "staging": "cross-env NODE_ENV=staging nuxt",
    "prod": "cross-env NODE_ENV=production nuxt",
    "analyze": "nuxt build --analyze",
    "build": "nuxt build",
    "start": "nuxt start",
    "start:dev": "cross-env NODE_ENV=development nuxt start",
    "generate": "nuxt generate ",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path ../../.gitignore .",
    "ls": "stylelint **/*.{vue,css,scss} --ignore-path ../../.gitignore",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path ../../.gitignore",
    "lint": "yarn lint:js",
    "lint:w": "esw --ext \".js,.ts,.vue\" --ignore-path ../../.gitignore . -w --color",
    "test": "jest --detectOpenHandles",
    "test:w": "jest --watch",
    "tsc": "tsc -w --noEmit -p tsconfig.tsc.json"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --ext \".js,.vue\" --fix --ignore-path ../../.gitignore .",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "author": "inforex",
  "license": "ISC",
  "dependencies": {
    "@nuxt/content": "^1.15.0",
    "@nuxt/vue-app": "^2.15.8",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.29.3",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/toast": "^3.3.1",
    "lottie-web": "^5.7.14",
    "nuxt": "^2.15.8",
    "nuxt-history-state": "^0.6.2",
    "nuxt-i18n": "^6.28.1",
    "socket.io-client": "~2.3.0",
    "swiper": "~5.4.5",
    "vue-awesome-swiper": "^4.1.1",
    "vue-webview-js-bridge": "^0.0.8",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/cssnano": "^1.0.1",
    "@nuxtjs/eslint-config": "~6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.3.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "@types/swiper": "^5.4.3",
    "@vue/test-utils": "^1.2.2",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-vue": "~7.20.0",
    "jest": "^27.3.1",
    "jest-nuxt-helper": "^1.3.0",
    "node-sass": "^6.0.1",
    "sass": "~1.32.13",
    "sass-loader": "~10.2.0",
    "vue": "^2.6.14",
    "vue-jest": "^3.0.7",
    "vue-runtime-helpers": "^1.1.2",
    "vue-template-compiler": "^2.6.14",
    "vuetify-dialog": "^2.0.17",
    "webpack": "~4.46.0"
  }
}
